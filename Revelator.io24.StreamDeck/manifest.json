{
  "$schema": "https://raw.githubusercontent.com/GeekyEggo/SharpDeck/master/src/SharpDeck.PropertyInspector/manifest-schema.json",
  "Name": "Revelator io24",
  "Version": "0.0.2",
  "Author": "oddbear",
  "UUID": "com.oddbear.revelator.io24",
  "Actions": [
    {
      "Name": "Experimental Hardware Mute Left Mic",
      "UUID": "com.oddbear.revelator.io24.keypad.microphone-hardware-mute",
      "Icon": "Images/Icons/Icon",
      "States": [
        {
          "Image": "Images/Plugin/mic_l_on",
          "Title": "Unmuted",
          "TitleAlignment": "bottom"
        },
        {
          "Image": "Images/Plugin/mic_l_off",
          "Title": "Muted",
          "TitleAlignment": "bottom"
        }
      ]
    },
    {
      "Name": "Route Change",
      "UUID": "com.oddbear.revelator.io24.keypad.route-change",
      "Icon": "Images/Icons/routing",
      "Tooltip": "A route changing action",
      "PropertyInspectorPath": "PropertyInspector/Keypads/RouteChangeKeypad.html",
      "States": [
        {
          "Image": "Images/Plugin/routing_on",
          "Title": "Route On",
          "TitleAlignment": "bottom"
        },
        {
          "Image": "Images/Plugin/routing_off",
          "Title": "Route Off",
          "TitleAlignment": "bottom"
        }
      ]
    },
    {
      "Name": "Headphone source",
      "UUID": "com.oddbear.revelator.io24.keypad.headphone-source",
      "Icon": "Images/Icons/headphones",
      "Tooltip": "A headphones source set action",
      "PropertyInspectorPath": "PropertyInspector/Keypads/HeadphonesSourceKeypad.html",
      "States": [
        {
          "Image": "Images/Plugin/headphones_on",
          "Title": "Headphones On",
          "TitleAlignment": "bottom"
        },
        {
          "Image": "Images/Plugin/headphones_off",
          "Title": "Headphones Off",
          "TitleAlignment": "bottom"
        }
      ]
    },
    {
      "Name": "Fat Channel Toggle",
      "UUID": "com.oddbear.revelator.io24.keypad.fatchannel-toggle",
      "Icon": "Images/Icons/fat",
      "Tooltip": "A Fat Channel toggle action",
      "PropertyInspectorPath": "PropertyInspector/Keypads/FatChannelToggleKeypad.html",
      "States": [
        {
          "Image": "Images/Plugin/fat_on",
          "Title": "Fat On",
          "TitleAlignment": "bottom"
        },
        {
          "Image": "Images/Plugin/fat_off",
          "Title": "Fat Off",
          "TitleAlignment": "bottom"
        }
      ]
    },
    {
      "Name": "Preset change",
      "UUID": "com.oddbear.revelator.io24.keypad.preset-change",
      "Icon": "Images/Icons/presets",
      "Tooltip": "A preset change action",
      "PropertyInspectorPath": "PropertyInspector/Keypads/PresetChangeKeypad.html",
      "States": [
        {
          "Image": "Images/Plugin/preset_on",
          "Title": "Preset On",
          "TitleAlignment": "bottom"
        },
        {
          "Image": "Images/Plugin/preset_off",
          "Title": "Preset Off",
          "TitleAlignment": "bottom"
        }
      ]
    },
    {
      "Name": "Input Volume Level",
      "UUID": "com.oddbear.revelator.io24.keypad.volume-level",
      "Icon": "Images/Icons/category",
      "Tooltip": "A volume change action",
      "PropertyInspectorPath": "PropertyInspector/Keypads/VolumeLevelKeypad.html",
      "States": [
        {
          "Image": "Images/Plugin/volume_on",
          "Title": "- dB",
          "TitleAlignment": "bottom"
        },
        {
          "Image": "Images/Plugin/volume_off",
          "Title": "- dB",
          "TitleAlignment": "bottom"
        }
      ]
    },
    {
      "Name": "Output Volume Level",
      "UUID": "com.oddbear.revelator.io24.keypad.output-level",
      "Icon": "Images/Icons/category",
      "Tooltip": "A output volume change action",
      "PropertyInspectorPath": "PropertyInspector/Keypads/OutputLevelKeypad.html",
      "States": [
        {
          "Image": "Images/Plugin/output_on",
          "Title": "- dB",
          "TitleAlignment": "bottom"
        },
        {
          "Image": "Images/Plugin/output_off",
          "Title": "- dB",
          "TitleAlignment": "bottom"
        }
      ]
    },
    {
      "Name": "Input Volume Level",
      "UUID": "com.oddbear.revelator.io24.encoder.volume-level",
      "Icon": "Images/Icons/category",
      "Tooltip": "A volume change dial",
      "PropertyInspectorPath": "PropertyInspector/Encoders/VolumeLevelEncoder.html",
      "Controllers": [ "Encoder" ],
      "Encoder": {
        "layout": "$B1",
        "TriggerDescription": {
          "Rotate": "Increase/Decrease",
          "Push": "Toggle mute"
        }
      },
      "States": [
        {
          "Image": "Images/Plugin/volume_on"
        }
      ]
    },
    {
      "Name": "Output Volume Level",
      "UUID": "com.oddbear.revelator.io24.encoder.output-level",
      "Icon": "Images/Icons/category",
      "Tooltip": "A output volume change dial",
      "PropertyInspectorPath": "PropertyInspector/Encoders/OutputLevelEncoder.html",
      "Controllers": [ "Encoder" ],
      "Encoder": {
        "layout": "$B1",
        "TriggerDescription": {
          "Rotate": "Increase/Decrease"
        }
      },
      "States": [
        {
          "Image": "Images/Plugin/output_on"
        }
      ]
    }
  ],
  "Category": "Revelator io24",
  "CategoryIcon": "Images/Icons/category",
  "CodePath": "Revelator.io24.StreamDeck.exe",
  "CodePathMac": "Revelator.io24.StreamDeck",
  "Description": "A Revelator io24 plugin for the Stream Deck",
  "Icon": "Images/Icons/category",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "6.4"
  },
  "OS": [
    {
      "MinimumVersion": "10.11",
      "Platform": "mac"
    },
    {
      "MinimumVersion": "10",
      "Platform": "windows"
    }
  ]
}
